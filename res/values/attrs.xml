<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="cardViewTheme" format="reference" />
    <attr name="listBackground" format="reference" />
    <attr name="drawerBackground" format="reference" />
    <attr name="colorIssueOpen" format="color" />
    <attr name="colorIssueClosed" format="color" />
    <attr name="colorIssueEditFab" format="reference" />
    <attr name="colorCommitAddition" format="color" />
    <attr name="colorCommitDeletion" format="color" />

    <attr name="sendIcon" format="reference" />
    <attr name="contentEditIcon" format="reference" />
    <attr name="sortIcon" format="reference" />
    <attr name="contentPictureIcon" format="reference" />
    <attr name="searchUserIcon" format="reference" />
    <attr name="searchRepoIcon" format="reference" />
    <attr name="userBookmarkIcon" format="reference" />
    <attr name="repoBookmarkIcon" format="reference" />
    <attr name="submoduleIcon" format="reference" />
    <attr name="dirIcon" format="reference" />
    <attr name="fileIcon" format="reference" />
    <attr name="commentsIcon" format="reference" />
    <attr name="languageSmallIcon" format="reference" />
    <attr name="forkSmallIcon" format="reference" />
    <attr name="starSmallIcon" format="reference" />
    <attr name="sizeSmallIcon" format="reference" />
    <attr name="dueSmallIcon" format="reference" />
    <attr name="milestoneSmallIcon" format="reference" />
    <attr name="downloadSmallIcon" format="reference" />
    <attr name="filesSmallIcon" format="reference" />

    <declare-styleable name="LabelBadgeView">
        <attr name="badgeSize" format="dimension"/>
        <attr name="badgeSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StyleableTextView">
        <attr name="font" format="enum">
            <enum name="regular" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="condensed" value="3" />
            <enum name="boldCondensed" value="4" />
        </attr>
        <attr name="allCaps" format="boolean"/>
    </declare-styleable>
</resources>
